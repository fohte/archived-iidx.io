{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "no-console": {
      "severity": "warning",
      "options": ["log"]
    },
    "interface-name": {
      "options": "never-prefix"
    },
    "object-literal-sort-keys": false,
    "max-classes-per-file": false,
    "ordered-imports": {
      "severity": "warning"
    },
    "no-empty-interface": false
  }
}
