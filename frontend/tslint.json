{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "no-console": {
      "options": ["log"]
    },
    "interface-name": {
      "options": "never-prefix"
    },
    "interface-over-type-literal": false,
    "object-literal-sort-keys": false,
    "max-classes-per-file": false,
    "no-empty-interface": false
  },
  "jsRules": {
    "object-literal-sort-keys": false
  }
}
